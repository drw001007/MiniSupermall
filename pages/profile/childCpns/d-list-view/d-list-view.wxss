/* pages/profile/childCpns/d-list-view/d-list-view.wxss */
.list-view {
  border-top: 36rpx solid #f8f8f8;
}

.list-item {
  height: 100%;
  line-height: 100rpx;
  background-color: #fff;
  font-size: 32rpx;
  margin-left: 30rpx;
  border-bottom: 1px solid #f8f8f8;
}

.list-item:last-child {
  border-bottom: none;
}

.image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}

.text {
  vertical-align: middle;
  margin-left: 16rpx;
}